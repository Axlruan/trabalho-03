using System;

class Program
{
    static void Main()
    {
        int[,] matriz = new int[5, 5];
        int valor = 1;

        for (int i = 0; i < 5; i++)
        {
            for (int j = 0; j < 5; j++)
            {
                matriz[i, j] = valor;
                valor++;
            }
        }

        Console.WriteLine("Valores das diagonais da matriz 5x5:");

        Console.WriteLine("\nDiagonal Principal:");
        for (int i = 0; i < 5; i++)
        {
            Console.WriteLine(matriz[i, i]);
        }

        Console.WriteLine("\nDiagonal SecundÃ¡ria:");
        for (int i = 0; i < 5; i++)
        {
            Console.WriteLine(matriz[i, 4 - i]);
        }
    }
}
